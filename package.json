{
  "name": "mikrotik-dashboard",
  "version": "1.0.0",
  "description": "MikroTik Router Management Dashboard",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:start": "concurrently \"npm run dev\" \"npm run dev:backend\"",
    "dev:full": "node scripts/dev-with-cleanup.mjs",
    "dev:backend": "cd server && npm run dev",
    "build": "tsc && vite build",
    "build:all": "npm run build && cd server && npm run build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@ant-design/plots": "^2.6.6",
    "@types/d3-force": "^3.0.10",
    "@types/react-syntax-highlighter": "^15.5.13",
    "antd": "^5.27.6",
    "d3-force": "^3.0.0",
    "dagre": "^0.8.5",
    "dayjs": "^1.11.18",
    "lucide-react": "^0.548.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-rnd": "^10.5.2",
    "react-syntax-highlighter": "^15.6.6",
    "reactflow": "^11.11.4",
    "remark-gfm": "^4.0.1",
    "socket.io-client": "^4.8.1",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/dagre": "^0.7.53",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "typescript": "^5.2.2",
    "vite": "^5.3.1"
  }
}
