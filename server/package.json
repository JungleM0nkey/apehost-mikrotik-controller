{
  "name": "mikrotik-dashboard-server",
  "version": "1.0.0",
  "description": "Backend API server for MikroTik Dashboard",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "migrate-config": "tsx scripts/migrate-config.ts",
    "validate-config": "tsx scripts/validate-config.ts",
    "backup-config": "tsx scripts/backup-config.ts",
    "restore-config": "tsx scripts/restore-config.ts",
    "list-backups": "tsx scripts/list-backups.ts"
