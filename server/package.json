    "boxen": "^7.1.1",
    "chalk": "^5.6.2",
    "chokidar": "^4.0.3",
    "cli-progress": "^3.12.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "node-routeros": "^1.6.9",
    "openai": "^6.7.0",
    "qrcode": "^1.5.4",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "socket.io": "^4.8.1",
    "ws": "^8.14.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/chokidar": "^1.7.5",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/node": "^20.9.0",
    "@types/qrcode": "^1.5.6",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.8",
    "tsx": "^4.6.2",
    "typescript": "^5.2.2"
  }
